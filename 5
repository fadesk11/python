# 1
a = int(input("Введите ваш возраст\n"))
if a < 17:
    print("Вы несовершеннолетний")
if a >= 18 and a <= 65:
        print("Вы трудоспособный человек")
if a > 65 :
        print("Вы пенсионер/Динозавр")
# 2
b = int(input("Введите стоимость покупки\n"))
if b <= 1000:
    print("Нема скидок, лошок")
if b > 1000 and b < 5000:
    print("Ваша скидка - 5%")
if b >= 5000:
    print("Ваша скидка - 10%")
# 3
f1 = int(input("Введите первое число\n"))
f2 = int(input("Введите второе число\n"))
f = input("Введите операцию, которую хотите провести над ними,+, -, *, /\n")
if f == "+":
    print(f1+f2)
if f == "-":
    print(f1-f2)
if f == "*":
    print(f1*f2)
if f == "/":
    print(f1/f2)
else:
    print("Учи математику")
# 4
chs = int(input("Введите ваше число"))
def check_number(n):
    if n % 2 == 0 and (n % 10 == 2 or n % 10 == 6):
        return True
    else:
        return False

print(check_number(chs))
